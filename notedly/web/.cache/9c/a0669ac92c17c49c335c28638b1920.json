 <ButtonAsLink onClick={() => {\n                            // 删除令牌\n                            localStorage.removeItem('token')\n                            // 清空应用的缓存\n                            client.resetStore()\n                            // 更新本地状态\n                            client.writeData({ data: { isLoggedIn: false } })\n                            // 把用户重定向到首页\n                            props.history.push('/')\n                        }}>Log Out</ButtonAsLink>\n                    ) : (\n                        <p>\n                            <Link to={'/signin'}>Sign In</Link> or{''}\n                            <Link to={'/signup'}>Sign Up</Link>\n                        </p>\n                    )\n                }\n\n            </UserState>\n        </HeaderBar>\n    );\n};\n\nexport default withRouter(Header);"},"lineCount":null}},"error":null,"hash":"af51771c12c0db10032541021abc75d4","cacheData":{"env":{}}}